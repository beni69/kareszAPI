{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-else-return": "warn",
        "no-unused-vars": "off",
        "camelcase": "warn",
        "quotes": ["error", "double"],
        "no-undef": "warn"
    }
}
